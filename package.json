{
  "name": "rapidtriage-me",
  "version": "2.0.0",
  "description": "YarlisAISolutions Browser Tools MCP Platform - Remote browser triage and debugging",
  "main": "functions/lib/index.js",
  "scripts": {
    "dev": "firebase emulators:start",
    "dev:functions": "cd functions && npm run build:watch",
    "build": "cd functions && npm run build",
    "build:watch": "cd functions && npm run build:watch",
    "deploy": "npm run build && firebase deploy",
    "deploy:functions": "npm run build && firebase deploy --only functions",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:rules": "firebase deploy --only firestore:rules,storage:rules",
    "logs": "firebase functions:log",
    "logs:api": "firebase functions:log --only api",
    "emulators": "firebase emulators:start",
    "emulators:export": "firebase emulators:start --export-on-exit=./emulator-data",
    "emulators:import": "firebase emulators:start --import=./emulator-data",
    "test": "node test/run-all-tests.js",
    "test:api": "node test/02-api/api-test-suite.js",
    "test:extension": "node test/04-extension/extension-test-suite.js",
    "test:integration": "node test/05-integration/integration-test-suite.js",
    "test:browser": "node test/03-browser/browser-test-suite.js",
    "test:functions": "cd functions && npm test",
    "test:organize": "bash test/organize-tests.sh",
    "test:clean": "rm -rf test/reports/*",
    "lint": "eslint src/**/*.ts && cd functions && npm run lint",
    "typecheck": "tsc --noEmit && cd functions && tsc --noEmit",
    "setup": "npm install && cd functions && npm install",
    "clean": "rm -rf functions/lib emulator-data"
  },
  "keywords": [
    "mcp",
    "firebase",
    "firebase-functions",
    "remote-debugging",
    "ai-tools",
    "yarlis",
    "browser-debugging"
  ],
  "author": "YarlisAISolutions",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "colors": "^1.4.0",
    "dotenv": "^17.2.1",
    "eslint": "^8.56.0",
    "eventsource": "^4.0.0",
    "firebase-tools": "^13.0.0",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2",
    "puppeteer": "^24.16.1",
    "typescript": "^5.3.3",
    "ws": "^8.18.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
